# Development Environment Variables Template
# Copy this to apps/server/.env for your second instance

# Development Environment Variables
NODE_ENV=development
PORT=3000

# Development Database (use your new Supabase project)
DATABASE_URL=postgresql://postgres:[YOUR_DEV_PASSWORD]@db.[YOUR_DEV_PROJECT_REF].supabase.co:5432/postgres

# Development Auth
BETTER_AUTH_URL=http://localhost:3000
BETTER_AUTH_SECRET=dev-secret-key-change-in-production

# Development Supabase
SUPABASE_URL=https://[YOUR_DEV_PROJECT_REF].supabase.co
SUPABASE_ANON_KEY=[YOUR_DEV_SUPABASE_ANON_KEY]
SUPABASE_SERVICE_ROLE_KEY=[YOUR_DEV_SUPABASE_SERVICE_ROLE_KEY]

# OAuth (can use same as production)
GOOGLE_CLIENT_ID=[YOUR_GOOGLE_CLIENT_ID]
GOOGLE_CLIENT_SECRET=[YOUR_GOOGLE_CLIENT_SECRET]
